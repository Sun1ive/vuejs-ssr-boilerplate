{
  "name": "vue-webpack-ssr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "[ $PREBUILD ] && npm run build || exit 0",
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "prestart": "npm run build",
    "analyze": "rimraf dist && npm run build:server && cross-env analyze=true NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build": "rimraf dist && npm run build:server && npm run build:client",
    "build:client": "cross-env myvar=test NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test": "rimraf coverage && jest",
    "test:acceptance": "rimraf tests/e2e/screenshots && cypress run --browser chrome",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./public"
  },
  "engines": {
    "node": ">= 10.0",
    "npm": ">= 5.0"
  },
  "keywords": [],
  "author": "Sergey Ustinov",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
